<!DOCTYPE html>
<html lang="fr">

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta http-equiv="X-UA-Compatible" content="ie=edge">
	<title>Map Runner</title>
	<link rel="stylesheet" type="text/css" href="./css/style-min.css">
</head>

<body>
	<div id="game">
		<!--       <canvas id="canvas" tabindex=1>You browser is too old to use canvas technologies.</canvas> -->
	</div>
	<div id="mapList">

		<div id="loading">
			<div id="loadBarOut">
				<div id="loadBarIn"></div>
			</div>
		</div>

		<p id="mapTitle" class="map offList">
			<span class="name">NAME</span>
			<span class="size">SIZE</span>
			<span class="time">BEST TIME</span>
		</p>
		<div id="maps"></div>
		<div id="btns" class="offList">
			<input id="btnCreate" type="button" value="CREATE">
			<input id="btnPlay" type="button" value="PLAY" disabled>
		</div>
	</div>


	<script type="text/javascript" src="./functions.js"></script>
	<script type="text/javascript" src="./engine/engine.js"></script>
	<script type="text/javascript" src="./engine/block.js"></script>
	<script type="text/javascript" src="./engine/bouncing_box.js"></script>
	<script type="text/javascript" src="./engine/camera.js"></script>
	<script type="text/javascript" src="./engine/door.js"></script>
	<script type="text/javascript" src="./engine/end.js"></script>
	<script type="text/javascript" src="./engine/player.js"></script>
	<script type="text/javascript" src="./engine/spawn.js"></script>
	<script type="text/javascript" src="./engine/spikes.js"></script>
	<script type="text/javascript" src="./engine/sword.js"></script>
	<script type="text/javascript" src="./engine/wall.js"></script>
	<script type="text/javascript" src="./engine/windows.js"></script>
	<script type="text/javascript" src="./game.js"></script>
	<script type="text/javascript">
		reloadMaps()
	</script>
</body>

</html>